{
  "name": "cypress-e2e",
  "version": "1.0.0",
  "description": "Cypress E2E Testsuite",
  "scripts": {
    "scripts": "cypress run",
    "test": "pnpm run scripts || pnpm run posttest",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "combine-reports": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "pretest": "pnpm run clean:reports",
    "posttest": "pnpm run combine-reports && pnpm run generate-report",
    "tests": "pnpm run pretest && cypress run && pnpm run posttest"
  },
  "keywords": [],
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^24.0.5"
  },
  "devDependencies": {
    "cypress": "13.3.0",
    "cypress-keycloak": "^2.0.1",
    "cypress-multi-reporters": "^1.6.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^3.8.3"
  },
  "repository": {}
}
